#include<stdio.h>
#include<conio.h>

int mu(int a,int b)
{
     if(b==1)
     return a;
     else
     return mu(a,b-1)*a;
}
     
int main()
{
    int n,x;
    printf("nhap so   ");
    scanf("%d",&n);
    printf("so mu   ");
    scanf("%d",&x);
    printf("%d",mu(n,x));
    getch();
    return 0;
}
